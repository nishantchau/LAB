@model List<DTO.LABURNUM.COM.LoginActivityModel>
@{int counter = 1;}
@if (Model.Count == 0)
{
    <div class="text-center">
        <img src="@LABURNUM.COM.Component.Constants.URL.WEBSITEURLWITHOUTSLASH/Images/no-record-found.png" />
    </div>
}
else
{
    <section class="panel">
        <header class="panel-heading">
            <strong>Number of Record Found : @Model.Count</strong>
            <span class="tools pull-right">
                <button type="button" onclick="DownloadRecord('Login Activity Record');" class="btn btn-primary adept-view">Download <i class="fa fa-download" aria-hidden="true"></i></button>
            </span>
        </header>

        <div class="panel-body" id="divTable">
            <div class="col-md-12">
                <div class="table-responsive">
                    <table class="table table-hover general-table responsive">
                        <thead>
                            <tr>
                                <th>S.No.</th>
                                <th>LoginActivityid</th>
                                <th>Login By</th>
                                <th>Login From</th>
                                <th>Login At</th>
                                <th>Logged Out At</th>
                                <th>User Type</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (DTO.LABURNUM.COM.LoginActivityModel item in @Model)
                            {
                                <tr>
                                    <td style="width: 17%;">@counter</td>
                                    <td style="width: 17%;">@item.LoginActivityId</td>
                                    <td style="width: 20%;">@item.StudentId</td>
                                    <td style="width: 19%;">@item.ClientName</td>
                                    <td style="width: 19%;">@item.LoginAt</td>
                                    <td style="width: 20%;">@item.LogoutAt</td>
                                    <td style="width: 20%;">@item.UserTypeText</td>
                                </tr>
                                counter++;
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </section>
}
<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel"></div>
