<body>
    <div id="divDomain" style="display: none">@LABURNUM.COM.Component.Constants.URL.WEBSITEURL</div>
    <div id="divUserType" style="display: none">@(new LABURNUM.COM.Component.SessionManagement().GetLoginBy())</div>
    <div id="divAcademicYear" style="display: none">@(new LABURNUM.COM.Component.SessionManagement().GetAcademicYearTableId())</div>
    <section id="container">
        <header class="header fixed-top clearfix">
            @{
                Html.RenderAction("Index", "Header");
            }
        </header>
        <div class="page-sidebar" id="ac_FixHeight">
            @{
                Html.RenderAction("Index", "SideBar");
            }
        </div>

        <section id="main-content">
            <section class="wrapper">
                @RenderBody()
                @{
                    Html.RenderAction("Index", "Footer");
                }
            </section>
        </section>
    </section>
</body>
</html>
<script type="text/javascript">
    $(document).ready(function () {
        var heightNoScrollBars = $(window).height();
        var heightNoScrollBarsFinal = heightNoScrollBars - 80;
        $("#ac_FixHeight").css("height", heightNoScrollBarsFinal);

        $("html").niceScroll({ styler: "fb", cursorcolor: "#000" });
        $("#ac_FixHeight").niceScroll();
    });
</script>
