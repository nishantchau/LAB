            </div>
        }


        @if (new LABURNUM.COM.Component.SessionManagement().GetLoginBy() == DTO.LABURNUM.COM.Utility.UserType.GetValue(DTO.LABURNUM.COM.Utility.EnumUserType.ADMIN))
        {
            <div class="col-md-3">
                <div class="button_home button_green dashboard_clock">
                    <i class="fa fa-envelope fa-3x" style="float: left; margin: 0px -8px 5px 8px"></i>
                    <div>SMS Balance :</div>
                    <div>@Model.AdminDashBoardModel.SMSBalance</div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="button_home button_orange dashboard_clock">
                    <i class="fa fa-users fa-3x" aria-hidden="true" style="float: left; margin: 0px -8px 5px 8px"></i>
                    <div>Staffs :</div>
                    <div>@(Model.AdminDashBoardModel.StaffSummary.TotalStaff)</div>
