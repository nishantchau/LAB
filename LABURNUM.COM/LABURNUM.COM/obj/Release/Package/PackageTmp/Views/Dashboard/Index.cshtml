                </div>
            </div>
            <div class="col-md-4">
                <div class="button_home button_voilet dashboard_clock">
                    <i class="fa fa-clock-o fa-3x" style="float: left; margin: 0px -8px 5px 8px"></i>
                    <div id="Date"></div>
                    <div>
                        <b id="hours"></b>
                        <b id="point">:</b>
                        <b id="min"></b>
                        <b id="point">:</b>
                        <b id="sec"></b>
                    </div>
                </div>
            </div>
            <div class="col-md-8 extra_margin_small">
                <section class="panel">
                    <header class="panel-heading">
                        Today Attendance Summary
                    </header>
                    <div class="panel-body">
                        <div class="table-responsive">
                            <table class="table table-hover general-table responsive">
                                <thead>
                                    <tr>
                                        <th width="2%">S.No.</th>
                                        <th width="1%">Class</th>
                                        <th width="3%">Section</th>
                                        <th width="1%">Students</th>
                                        <th width="5%">Mor. Present</th>
                                        <th width="5%">Mor. Absent</th>
                                        <th width="5%">Lunch Present</th>
                                        <th width="5%">Lunch Absent</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @{int counter = 1;}
                                    @foreach (DTO.LABURNUM.COM.AttendanceReporting.ClassModelForAttendance item in @Model.PrincipalDashBoardModel.AttendanceSummaryReportModel.DayWiseAttendanceSummaries[0].ClassModelsForAttendance)
                                    {
                                        <tr>
                                            <td width="2%">@counter</td>
                                            <td width="1%">@item.ClassName</td>
                                            <td width="3%">@item.SectionModelsForAttendance.Count</td>
                                            <td width="1%">@item.StudentInClass</td>
                                            <td width="5%">@item.MorningPresentStudentInClass</td>
                                            <td width="5%">@item.MorningAbsentStudentInClass</td>
                                            <td width="5%">@item.LunchPresentStudentInClass</td>
                                            <td width="5%">@item.LunchAbsentStudentInClass</td>
                                        </tr>
                                        counter++;
                                    }
                                    <tr>
                                        <td>Total</td>
                                        <td></td>
                                        <td></td>
                                        <td>@Model.PrincipalDashBoardModel.AttendanceSummaryReportModel.TotalStudentsInSchool</td>
                                        <td>@Model.PrincipalDashBoardModel.AttendanceSummaryReportModel.TotalStudentsMorningPresentCount</td>
                                        <td>@Model.PrincipalDashBoardModel.AttendanceSummaryReportModel.TotalStudentsMorningAbsentCount</td>
                                        <td>@Model.PrincipalDashBoardModel.AttendanceSummaryReportModel.TotalStudentsLunchPresentCount</td>
                                        <td>@Model.PrincipalDashBoardModel.AttendanceSummaryReportModel.TotalStudentsLunchAbsentCount</td>
                                    </tr>
                                    <tr>
                                        <td colspan="8" style="text-align: right;"><a href="@LABURNUM.COM.Component.Constants.URL.WEBSITEURLWITHOUTSLASH/Reporting/TodayAttendanceSummary">Click Here For Detailed Summary</a></td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </section>
            </div>
            <div class="col-md-4 extra_margin_small">
                <section class="panel">
                    <header class="panel-heading">
                        Student Summary of School @*<span class="tools pull-right">
                            <a class="fa fa-chevron-down" href="javascript:;"></a>
                        </span>*@
                    </header>
                    <div class="panel-body">
                        <div class="row">
                            <ul>
                                <li>Total Students : @Model.PrincipalDashBoardModel.StudentSummary.TotalAllStudentCount</li>
                                <li>Active Students : @Model.PrincipalDashBoardModel.StudentSummary.TotalActiveStudentCount</li>
                                <li>InActive Students : @Model.PrincipalDashBoardModel.StudentSummary.TotalInActiveStudentCount</li>
                            </ul>
                        </div>
                    </div>
                </section>
                <section class="panel">
                    <header class="panel-heading">
                        Staff Summary @*<span class="tools pull-right">
                            <a class="fa fa-chevron-down" href="javascript:;"></a>
                        </span>*@
                    </header>
                    <div class="panel-body">
                        <div class="row">
                            <ul>
                                <li>Total Staff : @Model.PrincipalDashBoardModel.StaffSummary.TotalStaff</li>
                                <li>Principle : @Model.PrincipalDashBoardModel.StaffSummary.PrincipleCount</li>
                                <li>Account : @Model.PrincipalDashBoardModel.StaffSummary.AccountCount</li>
                                <li>Faculty : @Model.PrincipalDashBoardModel.StaffSummary.FacultyCount</li>
                                <li>Class Teachers : @Model.PrincipalDashBoardModel.StaffSummary.ClassTeacherFaculty</li>
                                <li>Subject Teachers : @Model.PrincipalDashBoardModel.StaffSummary.SubjectTeacherFaculty</li>
                                <li>Left Staff : @Model.PrincipalDashBoardModel.StaffSummary.LeftStaff</li>
                            </ul>
                        </div>
                    </div>
                </section>
                <section class="panel">
                    <header class="panel-heading">
                        Upcoming Events 
                        @*<span class="tools pull-right">
                            <a class="fa fa-chevron-up" href="javascript:;"></a>
                        </span>*@
                    </header>
                    <div class="panel-body">
                        <div class="row">
                            <ul>
                                @if (Model.EventList.Count == 0)
                                {
                                    <li>No Upcoming Event</li>}
                                else
                                {

                                    foreach (DTO.LABURNUM.COM.EventModel item in Model.EventList)
                                    {
                                    <li>Event Date : @item.EventDate.Day/@item.EventDate.Month/@item.EventDate.Year</li>
                                    <li>Event Name : @item.EventName</li>
                                    <li>Event Type : @item.EventTypeText</li>
                                    }
                                }
                            </ul>
                        </div>
                    </div>
                </section>
            </div>
        }


    </div>
</section>
<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel"></div>
