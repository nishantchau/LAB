@model DTO.LABURNUM.COM.DashBoardModel

<div class="row">
    <div class="col-md-12">
        <!--breadcrumbs start -->
        <ul class="breadcrumb">
            <li><a href="@LABURNUM.COM.Component.Constants.URL.WEBSITEURLWITHOUTSLASH/Dashboard/Index"><i class="fa fa-home"></i>Home</a></li>
            <li class="active">Dashboard</li>
        </ul>
        <!--breadcrumbs end -->
    </div>
</div>
<section class="panel panel-body">
    <div class="row">
        @if (new LABURNUM.COM.Component.SessionManagement().GetLoginBy() == DTO.LABURNUM.COM.Utility.UserType.GetValue(DTO.LABURNUM.COM.Utility.EnumUserType.PARENT) || new LABURNUM.COM.Component.SessionManagement().GetLoginBy() == DTO.LABURNUM.COM.Utility.UserType.GetValue(DTO.LABURNUM.COM.Utility.EnumUserType.STUDENT))
        {
            <div class="col-md-3">
                <div class="button_home button_green dashboard_clock">
                    <i class="fa fa-envelope fa-3x" style="float: left; margin: 0px -8px 5px 8px"></i>
                    <div>
                        <span>Morning :</span>
                        @if (Model.StudentDashBoardModel.IsPresentInMorning)
                        {<span>Present</span> }
                        else
                        {<span>Absent</span>}
                    </div>
                    <div>
                        <span>Evening :</span>
                        @if (Model.StudentDashBoardModel.IsPresentAfterLunch)
                        {<span>Present</span> }
                        else
                        {<span>Absent</span>}
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="button_home button_green dashboard_clock">
                    <div>
                    </div>
                </div>
            </div>
            <div class="col-md-2">
                <div class="button_home button_green dashboard_clock">

                    <div>
                    </div>
                </div>
            </div>
            <div class="col-md-4">
                <div class="button_home button_voilet dashboard_clock">

                    <i class="fa fa-clock-o fa-3x" style="float: left; margin: 0px -8px 5px 8px"></i>
                    <div id="Date"></div>
                    <div>
                        <b id="hours"></b>
                        <b id="point">:</b>
                        <b id="min"></b>
                        <b id="point">:</b>
                        <b id="sec"></b>
                    </div>
                </div>
            </div>
            <div class="col-md-8 extra_margin_small">
                <section class="panel">
                    <header class="panel-heading">
                        <strong>Today Home Work</strong>
                        @*<span class="tools pull-right">
                <button type="button" onclick="DownloadRecord('Home Work Details');" class="btn btn-primary adept-view">Download <i class="fa fa-download" aria-hidden="true"></i></button>
            </span>*@
                    </header>
                    <div class="panel-body">
                        @if (@Model.StudentDashBoardModel.HomeWorkList.Count == 0)
                        {
                            <div class="table-responsive">
                                No Home Work Assigned For Today
                            </div>}
                        else
                        {
                            <div class="table-responsive">
                                <table class="table table-hover general-table responsive">
                                    <thead>
                                        <tr>
                                            <th>S.No.</th>
                                            <th>Faculty Name</th>
                                            <th>Subject</th>
                                            <th>HomeWork Details</th>
                                            @*<th>Assigned Date</th>*@
                                            <th>Attachment</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @{int counter = 1;}
                                        @foreach (DTO.LABURNUM.COM.HomeWorkModel item in @Model.StudentDashBoardModel.HomeWorkList)
                                        {
                                            <tr>
                                                <td style="width: 0%;">@counter</td>
                                                <td style="width: 17%;">@item.FacultyName</td>
                                                <td style="width: 12%;">@item.SubjectName</td>
                                                <td style="width: 15%;">@item.HomeWorkText</td>
                                                @*<td style="width: 15%;">@item.CreatedOn.ToShortDateString()</td>*@
                                                @if (item.Attachment != null && item.Attachment != "")
                                                {
                                                    <td style="width: 0%;"><a href="@LABURNUM.COM.Component.Constants.URL.WEBSITEURLWITHOUTSLASH/Uploads/HomeWork/@item.Attachment" target="_blank">Download</a></td>
                                                }
                                                else
                                                {
                                                    <td style="width: 0%;">-</td>
                                                }
                                            </tr>
                                                counter++;
                                        }
                                    </tbody>
                                </table>
                            </div>
                        }
                    </div>
                </section>
            </div>
            <div class="col-md-4 extra_margin_small">
                <section class="panel">
                    <header class="panel-heading">
                        <strong>Class Teacher </strong>
                        @*<span class="tools pull-right">
                            <a class="fa fa-chevron-up" href="javascript:;"></a>
                        </span>*@
                    </header>
                    <div class="panel-body">
                        <div class="row">
                            <ul>
                                <li>Name : @Model.StudentDashBoardModel.ClassTeacherName</li>
                                <li>Mobile : @Model.StudentDashBoardModel.ClassTeacherMobile</li>
                                <li>Email Id : @Model.StudentDashBoardModel.ClassTeacherEmail</li>
                            </ul>
                        </div>
                    </div>
                </section>
                <section class="panel">
                    <header class="panel-heading">
                        <strong>Upcoming Events </strong>@*<span class="tools pull-right">
                            <a class="fa fa-chevron-up" href="javascript:;"></a>
                        </span>*@
                    </header>
                    <div class="panel-body">
                        <div class="row">
                            <ul>
                                @if (Model.EventList.Count == 0)
                                {
                                    <li>No Upcoming Event</li>}
                                else
                                {

                                    foreach (DTO.LABURNUM.COM.EventModel item in Model.EventList)
                                    {
                                    <li>Event Date : @item.EventDate.Day/@item.EventDate.Month/@item.EventDate.Year</li>
                                    <li>Event Name : @item.EventName</li>
                                    <li>Event Type : @item.EventTypeText</li>
                                    }
                                }
                            </ul>
                        </div>
                    </div>
                </section>
                @if (new LABURNUM.COM.Component.SessionManagement().GetLoginBy() == DTO.LABURNUM.COM.Utility.UserType.GetValue(DTO.LABURNUM.COM.Utility.EnumUserType.PARENT))
                {
                    <section class="panel">
                        <header class="panel-heading">
                            <strong> Last Fee Paid Details  </strong> @*<span class="tools pull-right">
                                <a class="fa fa-chevron-up" href="javascript:;"></a>
                            </span>*@
                        </header>
                        <div class="panel-body">
                            <div class="row">
                                <ul>
                                    <li>Paid Date : @Model.StudentDashBoardModel.LastPaidDetails.LastPaidDate.Day/@Model.StudentDashBoardModel.LastPaidDetails.LastPaidDate.Month/@Model.StudentDashBoardModel.LastPaidDetails.LastPaidDate.Year</li>
                                    <li>Receipt No. : @Model.StudentDashBoardModel.LastPaidDetails.ReceiptNumber</li>
                                    <li>Cash Paid : @Model.StudentDashBoardModel.LastPaidDetails.LastCashPaidAmount</li>
                                    <li>Cheque Paid: @Model.StudentDashBoardModel.LastPaidDetails.LastChequePaidAmount</li>
                                    <li>Pending Fee : @Model.StudentDashBoardModel.LastPaidDetails.PendingFee</li>
                                </ul>
                            </div>
                        </div>
                    </section>
                }
            </div>
        }


        @if (new LABURNUM.COM.Component.SessionManagement().GetLoginBy() == DTO.LABURNUM.COM.Utility.UserType.GetValue(DTO.LABURNUM.COM.Utility.EnumUserType.ACCOUNT))
        {
            <div class="col-md-3">
                <div class="button_home button_green dashboard_clock">
                    @*<i class="fa fa-envelope fa-3x" style="float: left; margin: 0px -8px 5px 8px"></i>
                    <span>Morning :</span>
                    @if (Model.StudentDashBoardModel.IsPresentInMorning)
                    {<span>Present</span> }
                    else
                    {<span>Absent</span>}
                    <span>Evening :</span>
                    @if (Model.StudentDashBoardModel.IsPresentAfterLunch)
                    {<span>Present</span> }
                    else
                    {<span>Absent</span>}*@
                </div>
            </div>
            <div class="col-md-3">
                <div class="button_home button_green dashboard_clock">
                    <div>
                    </div>
                </div>
            </div>
            <div class="col-md-2">
                <div class="button_home button_green dashboard_clock">

                    <div>
                    </div>
                </div>
            </div>
            <div class="col-md-4">
                <div class="button_home button_voilet dashboard_clock">

                    <i class="fa fa-clock-o fa-3x" style="float: left; margin: 0px -8px 5px 8px"></i>
                    <div id="Date"></div>
                    <div>
                        <b id="hours"></b>
                        <b id="point">:</b>
                        <b id="min"></b>
                        <b id="point">:</b>
                        <b id="sec"></b>
                    </div>
                </div>
            </div>
            <div class="col-md-8 extra_margin_small">
                <section class="panel">
                    <header class="panel-heading">
                        Pending Cheque Details (Last @LABURNUM.COM.Component.Constants.DEFAULTVALUE.CHEQUEDETAILDAYS Days)
                         <span class="tools pull-right">
            <button type="button" onclick="DownloadRecord('Pending Cheque List');" class="btn btn-primary adept-view">Download <i class="fa fa-download" aria-hidden="true"></i></button>
        </span>
                    </header>
                    <div class="panel-body" id="divTable">
                        @{long Chequestatus = DTO.LABURNUM.COM.Utility.ChequeStatusMaster.GetChequeStatusMasterId(DTO.LABURNUM.COM.Utility.EnumChequeStatusMaster.SUBMITTED);}
                        @if (@Model.AccountDashBoardModel.StudentFeeDetails.Where(x => x.ChequeStatus == Chequestatus).ToList().Count == 0)
                        {
                            <div class="table-responsive">
                                No Cheque Details Found Whose Status is Pending
                            </div>
                        }
                        else
                        {
                            <div class="table-responsive">
                                <table class="table table-hover general-table responsive table-fixed">
                                    <thead>
                                        <tr>
                                            <th width="2%">S.No.</th>
                                            <th width="12%">Class / Section</th>
                                            @*<th width="10%">Section</th>*@
                                            <th width="20%">Student Name</th>
                                            <th width="17%">Chq. No.</th>
                                            <th width="17%">Chq. Date</th>
                                            <th width="8%">Amount</th>
                                            <th width="23%"></th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @{int counter = 1;}
                                        @foreach (DTO.LABURNUM.COM.StudentFeeDetailModel item in @Model.AccountDashBoardModel.StudentFeeDetails.Where(x => x.ChequeStatus == 3).ToList())
                                        {
                                            <tr>
                                                <td style="width: 9%;">@counter</td>
                                                <td style="width: 12%;">@item.ClassName / @item.SectionName</td>
                                                @*<td style="width: 10%;"></td>*@
                                                <td style="width: 20%;">@item.StudentName</td>
                                                <td style="width: 17%;">@item.ChequeNumber</td>
                                                <td style="width: 17%;">@item.ChequeDate.GetValueOrDefault().Day/@item.ChequeDate.GetValueOrDefault().Month/@item.ChequeDate.GetValueOrDefault().Year</td>
                                                <td style="width: 11%;">@item.ChequePaidAmount</td>
                                                @if (item.ChequeStatus == DTO.LABURNUM.COM.Utility.ChequeStatusMaster.GetChequeStatusMasterId(DTO.LABURNUM.COM.Utility.EnumChequeStatusMaster.SUBMITTED))
                                                {
                                                    <td width="23%">
                                                        <button id="btnUpdate" class="btn btn-success adept-view" data-toggle="modal" data-target="#myModal" onclick="UpdateChequeStatusPoupUp('@item.StudentFeeDetailId')">Update Status</button></td> 
                                                }
                                                else
                                                {
                                                    <td width="23%">
                                                        <button class="btn btn-success adept-view" disabled>Update Status</button>
                                                    </td> 
                                                }
                                            </tr>
                                                counter++;
                                        }
                                    </tbody>
                                </table>
                            </div>
                        }
                    </div>
                </section>
            </div>
            <div class="col-md-4 extra_margin_small">
                <section class="panel">
                    <header class="panel-heading">
                        Cheque Details @*<span class="tools pull-right">
                            <a class="fa fa-chevron-down" href="javascript:;"></a>
                        </span>*@
                    </header>
                    <div class="panel-body">
                        <div class="row">
                            <ul>
                                <li>Total Received Cheque : @Model.AccountDashBoardModel.ChequeDetails.TotalChequeCount </li>
                                <li>Total Pending Cheque : @Model.AccountDashBoardModel.ChequeDetails.TotalPendingCheque </li>
                                <li>Total Bounce Cheque : @Model.AccountDashBoardModel.ChequeDetails.TotalBounceCheque </li>
                                <li>Total Cleared Cheque : @Model.AccountDashBoardModel.ChequeDetails.TotalClearedCheque </li>
                            </ul>
                        </div>
                    </div>
                </section>
                <section class="panel">
                    <header class="panel-heading">
                        Upcoming Events @*<span class="tools pull-right">
                            <a class="fa fa-chevron-up" href="javascript:;"></a>
                        </span>*@
                    </header>
                    <div class="panel-body">
                        <div class="row">
                            <ul>
                                @if (Model.EventList.Count == 0)
                                {
                                    <li>No Upcoming Event</li>}
                                else
                                {

                                    foreach (DTO.LABURNUM.COM.EventModel item in Model.EventList)
                                    {
                                    <li>Event Date : @item.EventDate.Day/@item.EventDate.Month/@item.EventDate.Year</li>
                                    <li>Event Name : @item.EventName</li>
                                    <li>Event Type : @item.EventTypeText</li>
                                    }
                                }
                            </ul>
                        </div>
                    </div>
                </section>
            </div>
        }



        @if (new LABURNUM.COM.Component.SessionManagement().GetLoginBy() == DTO.LABURNUM.COM.Utility.UserType.GetValue(DTO.LABURNUM.COM.Utility.EnumUserType.FACULTY))
        {
            <div class="col-md-3">
                <div class="button_home button_green dashboard_clock">
                    @*<i class="fa fa-envelope fa-3x" style="float: left; margin: 0px -8px 5px 8px"></i>
                    <span>Morning :</span>
                    @if (Model.StudentDashBoardModel.IsPresentInMorning)
                    {<span>Present</span> }
                    else
                    {<span>Absent</span>}
                    <span>Evening :</span>
                    @if (Model.StudentDashBoardModel.IsPresentAfterLunch)
                    {<span>Present</span> }
                    else
                    {<span>Absent</span>}*@
                </div>
            </div>
            <div class="col-md-3">
                <div class="button_home button_green dashboard_clock">
                    <div>
                    </div>
                </div>
            </div>
            <div class="col-md-2">
                <div class="button_home button_green dashboard_clock">

                    <div>
                    </div>
                </div>
            </div>
            <div class="col-md-4">
                <div class="button_home button_voilet dashboard_clock">

                    <i class="fa fa-clock-o fa-3x" style="float: left; margin: 0px -8px 5px 8px"></i>
                    <div id="Date"></div>
                    <div>
                        <b id="hours"></b>
                        <b id="point">:</b>
                        <b id="min"></b>
                        <b id="point">:</b>
                        <b id="sec"></b>
                    </div>
                </div>
            </div>
            <div class="col-md-8 extra_margin_small">
                <section class="panel">
                    <header class="panel-heading">
                        NONE
                    </header>
                    <div class="panel-body">
                    </div>
                </section>
            </div>
            <div class="col-md-4 extra_margin_small">
                @if (Model.IsClassTeacher)
                {
                    <section class="panel">
                        <header class="panel-heading">
                            Attendance Summary For Today <span class="tools pull-right">
                                <a class="fa fa-chevron-down" href="javascript:;"></a>
                            </span>
                        </header>
                        <div class="panel-body">
                            <div class="row">
                                <ul>
                                    <li>Morning Present Student : @Model.FacultyDashBoardModel.ClassAttendanceStatus.MorningPresentStudentCount</li>
                                    <li>Morning Absent Student : @Model.FacultyDashBoardModel.ClassAttendanceStatus.MorningAbsentStudentCount</li>
                                    <li>Lunch Present Student : @Model.FacultyDashBoardModel.ClassAttendanceStatus.LunchPresentStudentCount</li>
                                    <li>Lunch Absent Student : @Model.FacultyDashBoardModel.ClassAttendanceStatus.LunchAbsentStudentCount</li>
                                </ul>
                            </div>
                        </div>
                    </section>
                }
                <section class="panel">
                    <header class="panel-heading">
                        Upcoming Events <span class="tools pull-right">
                            <a class="fa fa-chevron-up" href="javascript:;"></a>
                        </span>
                    </header>
                    <div class="panel-body" style="display: none;">
                        <div class="row">
                            <ul>
                                @if (Model.EventList.Count == 0)
                                {
                                    <li>No Upcoming Event</li>}
                                else
                                {

                                    foreach (DTO.LABURNUM.COM.EventModel item in Model.EventList)
                                    {
                                    <li>Event Date : @item.EventDate.Day/@item.EventDate.Month/@item.EventDate.Year</li>
                                    <li>Event Name : @item.EventName</li>
                                    <li>Event Type : @item.EventTypeText</li>
                                    }
                                }
                            </ul>
